{
  "entrypoint": "index.html",
  "shell": "src\\whip-app.html",
  "fragments": [
    "src\\whip-app.html",
    "src\\el-audio.html",
    "src\\el-gyroscope.html"
  ],
  "sources": [
    "audio\\**\\*",
    "static\\**\\*",
    "images\\**\\*",
    "src\\**\\*",
    "*.php",
    "index.html",
    "src\\whip-app.html",
    "src\\whip-app.html",
    "src\\el-audio.html",
    "src\\el-gyroscope.html"
  ],
  "extraDependencies": [
    "bower_components\\webcomponentsjs\\*.js",
    "bower_components\\webcomponentsjs\\*.js",
    "!bower_components\\webcomponentsjs\\gulpfile.js",
    "manifest.json"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es5-bundled"
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es6-bundled"
    },
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled"
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}